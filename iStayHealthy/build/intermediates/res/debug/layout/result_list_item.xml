<?xml version="1.0" encoding="utf-8"?>
<TableLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:padding="6dp"
  android:stretchColumns="0"
  android:layout_width="fill_parent"
  android:layout_height="wrap_content">
	<TableRow 
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content">
		  <TableLayout 
		  	android:id="@+id/resultListTable"
			android:layout_marginLeft="5dip"
			android:layout_marginRight="5dip"
			android:layout_marginTop="3dip"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content">	  
		  	<TableRow 
		  		android:id="@+id/resultDateRow"
				android:layout_marginLeft="5dip"
				android:layout_marginRight="5dip"
		  		android:layout_span="3"
		  		android:layout_width="wrap_content"
		  		android:layout_height="wrap_content">
		  		<TextView android:id="@+id/resultsDate"
			       android:focusableInTouchMode="false"
			       android:focusable="false"	  
				   android:gravity="left"
				   android:textSize="10sp"
			   	   android:textColor="@color/darkdarkgray"
				   android:textStyle="italic"
				   android:layout_width="wrap_content"
				   android:layout_height="wrap_content"/>
		  	</TableRow>		  	
		  	<TableRow 
		  		android:layout_width="wrap_content"
		  		android:layout_height="wrap_content">
	        <TextView
	           android:id="@+id/CD4Title"
		       android:focusableInTouchMode="false"
		       android:focusable="false"	  
			   android:gravity="left"
			   android:textSize="14sp"
			   android:textStyle="bold"
			   android:textColor="@color/textcolour"
				android:layout_marginLeft="5dip"
				android:layout_marginRight="5dip"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"/>
	        <TextView
	           android:id="@+id/CD4PercentTitle"
		       android:focusableInTouchMode="false"
		       android:focusable="false"	  
			   android:gravity="left"
			   android:textSize="14sp"
			   android:textStyle="bold"
			   android:textColor="@color/textcolour"
				android:layout_marginLeft="5dip"
				android:layout_marginRight="5dip"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"/>
	        <TextView
	           android:id="@+id/ViralLoadTitle"
		       android:focusableInTouchMode="false"
		       android:focusable="false"	  
			   android:gravity="left"
			   android:textSize="14sp"
			   android:textStyle="bold"
			   android:textColor="@color/textcolour"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"/>
	        </TableRow>
	        
	        <TableRow 
	        	android:layout_width="wrap_content"
		  		android:layout_height="wrap_content">
	        <TextView 
	           android:id="@+id/CD4Count"
		       android:focusableInTouchMode="false"
		       android:focusable="false"	  
			   android:gravity="left"
			   android:textSize="12sp"
			   android:textStyle="bold"
				android:layout_marginLeft="5dip"
				android:layout_marginRight="5dip"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"/>
	
	        <TextView 
	           android:id="@+id/CD4Percent"
		       android:focusableInTouchMode="false"
		       android:focusable="false"	  
			   android:gravity="left"
			   android:textSize="12sp"
			   android:textStyle="bold"
				android:layout_marginLeft="5dip"
				android:layout_marginRight="5dip"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"/>
	
	        <TextView 
	           android:id="@+id/ViralLoad"
		       android:focusableInTouchMode="false"
		       android:focusable="false"	  
			   android:gravity="left"
			   android:textSize="12sp"
			   android:textStyle="bold"
				android:layout_marginLeft="10dip"
				android:layout_marginRight="10dip"
			   android:layout_width="wrap_content"
			   android:layout_height="wrap_content"/>
	
			</TableRow>
		</TableLayout>
	<ImageView android:id="@+id/editResultsImage"
	  android:contentDescription="@string/view"		        
	  android:paddingRight="6dip"
	  android:layout_gravity="center_vertical|center_horizontal"		
	  android:layout_width="wrap_content"
	  android:layout_height="wrap_content"
	  android:focusableInTouchMode="false"
	  android:focusable="false"
	  android:src="@drawable/nextitem"/>    
	</TableRow>
</TableLayout>
