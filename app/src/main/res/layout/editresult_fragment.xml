					android:layout_marginRight="6dip"            
		    		android:textSize="15sp"
		    		android:layout_width="wrap_content"    
		    		android:layout_height="wrap_content"/>
    		</TableRow> 
    		
    		
    		<TableRow 
    		    android:id="@+id/HepCRow"
    		 	android:layout_width="fill_parent"
